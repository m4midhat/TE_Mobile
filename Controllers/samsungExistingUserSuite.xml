<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="B2B Samsung App Smoke Suite">

    <listeners>
        <listener class-name="org.te.app.listener.extentReportListenerSamsung" />
        <listener class-name="org.te.app.analyzer.AnnotationTransformer" />
    </listeners>

    <test verbose="2" preserve-order="true" name="Samsung Android App">
        <classes>
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.HomeTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.SearchTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.FashionRetailTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.FoodAndDrinksTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.BeautyAndFitnessTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.ProfileDetailsTest"/>
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.HomeUpdatedCurrencyTest" />
            <class name="org.te.app.android.tests.samsung.appTests.existingUser.FavouritesTest" />
        </classes>
    </test>
</suite>